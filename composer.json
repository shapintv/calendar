{
    "name": "shapin/calendar",
    "description": "Calendar related tools.",
    "license": "MIT",
    "keywords": ["calendar", "ics"],
    "authors": [
        {
            "name": "Olivier Dolbeaua",
            "email": "olivier@bbnt.me"
        }
    ],
    "require": {
        "sabre/vobject": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Shapin\\Calendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapin\\Calendar\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "symfony/var-dumper": "^4.2",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    }
}
